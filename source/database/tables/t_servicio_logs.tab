create table T_SERVICIO_LOGS
(
  id_servicio_log NUMBER not null,
  id_servicio     NUMBER(3),
  parametros      CLOB,
  respuesta       CLOB
)
;
comment on table T_SERVICIO_LOGS
  is 'Logs de Servicios Web';
comment on column T_SERVICIO_LOGS.id_servicio_log
  is 'Identificador del log';
comment on column T_SERVICIO_LOGS.id_servicio
  is 'Identificador del servicio';
comment on column T_SERVICIO_LOGS.parametros
  is 'Parametros recibidos';
comment on column T_SERVICIO_LOGS.respuesta
  is 'Respuesta enviada';
alter table T_SERVICIO_LOGS
  add constraint PK_SERVICIO_LOGS primary key (ID_SERVICIO_LOG);
alter table T_SERVICIO_LOGS
  add constraint FK_SERVICIO_LOGS_SERVICIOS foreign key (ID_SERVICIO)
  references T_SERVICIOS (ID_SERVICIO);

